		<dcterms:identifier>http://data.europa.eu/dr8/SharedLegalContentGrouping_Properties</dcterms:identifier>
		<skos:prefLabel xml:lang="en"><![CDATA[Properties of Shared Legal Content]]></skos:prefLabel>
		<skos:topConceptOf rdf:resource="http://data.europa.eu/dr8" />
