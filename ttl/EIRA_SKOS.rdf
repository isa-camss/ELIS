	<skos:Concept rdf:about="http://data.europa.eu/dr8/PreservationOfInformation">
		<skos:notation><![CDATA[Preservation of information]]></skos:notation>
		<dcterms:identifier>http://data.europa.eu/dr8/PreservationOfInformation</dcterms:identifier>
